# RT-Thread building script for bridge

import rtconfig
from building import *

cwd     = GetCurrentDir()
CPPPATH = [cwd, cwd + '/hal_os']

src =  Split('''
    sto_com_board.c
    hal_os/hal_os.c
    hal_os/hal_os_test.c
    '''
    )

group = DefineGroup('sto_com_board', src, depend = ['APP_STO_COM_BOARD'], CPPPATH = CPPPATH)

Return('group')
