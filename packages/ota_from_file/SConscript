# RT-Thread building script for bridge

import rtconfig
from building import *

cwd     = GetCurrentDir()
CPPPATH = [cwd, str(Dir('#'))]
src     = Split("""
                ota_from_file.c
                """)

group = DefineGroup('ota_from_file', src, depend = ['PKG_USING_OTA_FROM_FILE'], CPPPATH = CPPPATH)

Return('group')
