# RT-Thread building script for bridge

import rtconfig
from building import *

cwd     = GetCurrentDir()
CPPPATH = [cwd, str(Dir('#'))]
src     = Split("""
                sysinfo.c
                """)

group = DefineGroup('sysinfo', src, depend = ['PKG_USING_SYSINFO'], CPPPATH = CPPPATH)

Return('group')
